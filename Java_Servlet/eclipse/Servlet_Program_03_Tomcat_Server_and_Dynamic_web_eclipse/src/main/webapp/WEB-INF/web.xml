<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd" id="WebApp_ID" version="6.0">
  <display-name>Servlet_Program_03_Tomcat_Server_and_Dynamic_web_eclipse</display-name>
  <servlet>
    <servlet-name>myservlet</servlet-name>
    <servlet-class>com.example.backend.MyServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>myservlet</servlet-name> // this name same as servlet tag servlet-name tag value
    <url-pattern>/vvv</url-pattern> // in browser ult add this "/vvv" this is doGet method, this open new page name is vvv
  </servlet-mapping>
</web-app>